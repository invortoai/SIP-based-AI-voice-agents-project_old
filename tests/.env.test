# Jest test environment overrides (safe defaults)
NODE_ENV=test
PUBLIC_BASE_URL=http://localhost
REDIS_URL=redis://localhost:6379

# Telephony concurrency (kept small for tests)
TELEPHONY_GLOBAL_MAX_CONCURRENCY=10
TELEPHONY_PER_CAMPAIGN_MAX_CONCURRENCY=5
TELEPHONY_SEMAPHORE_TTL_SEC=60

# Ensure services skip app.listen() guards
JEST_WORKER_ID=1