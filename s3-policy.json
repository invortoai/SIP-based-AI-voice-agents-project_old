{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:DeleteObject",
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::invorto-production-recordings/*",
                "arn:aws:s3:::invorto-production-recordings",
                "arn:aws:s3:::invorto-production-transcripts/*",
                "arn:aws:s3:::invorto-production-transcripts",
                "arn:aws:s3:::invorto-production-metrics/*",
                "arn:aws:s3:::invorto-production-metrics"
            ]
        }
    ]
}